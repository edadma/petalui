{
  "name": "asterui",
  "version": "0.12.49",
  "description": "React UI component library with DaisyUI",
  "homepage": "https://asterui.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edadma/asterui.git"
  },
